<!-- header -->
<% include ../partials/header %>
<!-- end header -->
                <!-- /.navbar-top-links -->
                <div class="navbar-default sidebar" role="navigation">
                    <div class="sidebar-nav navbar-collapse">
                        <!-- menu -->
                        <% include ../partials/menu %>
                        <!-- end header -->
                    </div>
                </div>
            </nav>
            <div id="page-wrapper">
                <div class="row">
                    <div class="col-lg-12">
                        <h4 class="page-header">Partner</h4>
                    </div>
                </div>
                <!-- CONTENT -->
                <!-- /.row -->
                <div class="row">
                  <div class="col-lg-12">
                      <div class="panel panel-default">
                          <div class="panel-heading">
                                <a href="/dashboard/partner/new"><button type="button" class="btn btn-success" ><i class="fa fa-plus" aria-hidden="true"></i>&nbsp; Add</button></a>
                                <!-- <ul class="list-inline">
                                        <li>SEARCH &nbsp; <input type="text" name="search" id="search-admin" placeholder="Search.."></li>
                                        <li>FROM DATE &nbsp; <input type="date" id="start-date"></li>
                                        <li>TO DATE &nbsp; <input type="date" id="end-date"></li>
                                        <li><button type="button" class="btn btn-primary"><i class="fa fa-search" aria-hidden="true"></i> &nbsp;Search </button></li>
                                </ul>   -->
                            </div>
                          <!-- /.panel-heading -->
                          <div class="panel-body">
                              <div class="dataTable_wrapper">
                                  <table class="table table-striped table-bordered table-hover" id="dataTables-example">
                                      <thead>
                                          <tr>
                                              <th>No</th>
                                              <th>IMAGE</th>
                                              <th>NAME</th>
                                              <th>DESCRIPTION</th> 
                                              <th>ACTION</th>
                                          </tr>
                                      </thead>
                                      <tbody>
                                           <% if(data) {%>
                                            <% data.forEach(function(partner,index){ %>
                                                <tr>
                                                    <td><%= index + 1 %></td>
                                                    <td><img src="/images/partner/<%=  partner.image %>" class="img-circle img-responsive" alt="product image" width="50" height="100%" style="margin: 0 auto;"> </td>
                                                    <td><%=  partner.name %></td>
                                                    <td><%=  partner.description %></td>
                                                    <td>
                                                        <ul class="list-inline">
                                                            <li>
                                                                <a href="/dashboard/partner/edit/<%= partner.id %>">
                                                                    <input type="submit"  value="Update" class="btn btn-warning">
                                                                </a>
                                                            </li>
                                                            <li>
                                                                <form class="d-inline" action="/dashboard/partner/delete/<%= partner.id %>" method="post">
                                                                    <input type="submit"  value="Delete" class="btn btn-danger">
                                                                </form>
                                                            </li>
                                                        </ul>
                                                    </td>
                                                </tr>
                                            <% }) %>
                                            <% }%>
                                      </tbody>
                                  </table>
                                  <ul class="pagination pull-right">
                                    <li class="previous"><a href="#">Prev</a></li>
                                    <li><a href="#">1</a></li>
                                    <li><a href="#">2</a></li>
                                    <li><a href="#">3</a></li>
                                    <li class="next"><a href="#">Next</a></li>
                                  </ul>
                              </div>
                          </div>
                          <!-- /.panel-body -->
                      </div>
                      <!-- /.panel -->
                  </div>
                  <!-- /.col-lg-12 -->
                </div>
                <!-- /.row -->
                <!-- END CONTENT -->
            </div>   
<!-- header -->
<% include ../partials/footer %>
<!-- end header -->
       